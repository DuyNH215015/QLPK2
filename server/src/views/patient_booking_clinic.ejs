<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Booking clinic</title>

  <!-- Latest compiled and minified CSS -->
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/css/bootstrap.min.css">

  <!-- jQuery library -->
  <script src="https://cdn.jsdelivr.net/npm/jquery@3.6.4/dist/jquery.slim.min.js"></script>

  <!-- Popper JS -->
  <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js"></script>

  <!-- Latest compiled JavaScript -->
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/js/bootstrap.bundle.min.js"></script>
</head>

<form action="/booking-state-clinic" method="POST">
<div class="form-group col-md-3">
  <label for="inputClinic">Nơi đăng ký khám</label>
  <select name="clinicId" id="clinicId">
    <% clinics.forEach(function(clinic) { %>
      <option value="<%= clinic.id %>"><%= clinic.Clinic_name %></option>
    <% }); %>
  </select>
</div>
<div>
  <input type="submit" value="Tiếp theo">
</div>
</form>
